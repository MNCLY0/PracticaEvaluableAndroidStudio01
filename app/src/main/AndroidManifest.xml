<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/logoapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logoapp_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme_night"
        tools:targetApi="31">
        <activity
            android:name=".CrearCuentaActivity"
            android:exported="false"
            android:label="@string/title_activity_crear_cuenta"
            android:theme="@style/AppTheme_night" />
        <activity
            android:name=".HobbieInfoActivity"
            android:exported="false"
            android:label="@string/title_activity_view_hobbie_info"
            android:theme="@style/AppTheme_night" />
        <activity
            android:name=".Login"
            android:exported="true"
            android:theme="@style/AppTheme_night">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainMenu"
            android:exported="false" />
        <activity
            android:name=".AddHobbieAppActivity"
            android:exported="false" />
    </application>

</manifest>